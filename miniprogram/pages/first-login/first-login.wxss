page {
  height: 100%;
}

.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

/* 首页按钮的样式 */
.button {
  width: 55%;
  height: 80rpx;
  background: #889fd2;
  margin-top: 25px;
  font-size: 38rpx;
  color: white;
  font-weight: 500;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 20rpx;
}

.bottom{
  z-index: 0;
  position: absolute;
  bottom: 20rpx;
  font-size: 26rpx;
  text-align: center;
  width: 100%;
  color: grey;
}
